body {
    background-color: #c2bdbb;
    font-family: Arial;
}
.mainContainer {
    margin: 5px;
}
#title {
    display: block;
    font-size: 20px;
    font-weight: bold;
    padding-top: 5px;
    padding-bottom: 5px;
}
.leftColumn {
    display: inline-block;
    float: left;
    height: 100%;
    width: 66%;
    margin-right: 10px;
    margin-top: 5px;
}
.rightColumn {
    display: inline-block;
    width: 33.3%;
    height: 100%;
    margin-top: 5px;
}
.boxElement {
    background: -webkit-linear-gradient(#FAFAFA, #BEBEBE); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#FAFAFA, #BEBEBE); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#FAFAFA, #BEBEBE); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#FAFAFA, #BEBEBE); /* Standard syntax */
    -webkit-box-sizing : border-box;‌​
    -moz-box-sizing : border-box;
    box-sizing : border-box;
    padding: 15px;
    height: auto;
    width: 100%;
    -webkit-box-shadow: 0px 0px 5px #545252;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
    -moz-box-shadow: 0px 0px 5px #545252;  /* Firefox 3.5 - 3.6 */
    box-shadow: 0px 0px 5px #545252;
}
.boldText {
    font-weight: bold;
}
#normalBox {
    margin-bottom: 10px;
}
#boxElementTop {
    background: -webkit-linear-gradient(#FAFAFA, #BEBEBE); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#FAFAFA, #BEBEBE); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#FAFAFA, #BEBEBE); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#FAFAFA, #BEBEBE); /* Standard syntax */
    -webkit-box-sizing : border-box;‌​
    -moz-box-sizing : border-box;
    box-sizing : border-box;
    padding: 15px;
    height: auto;
    width: 100%;
    margin-bottom: 20px;
    -webkit-box-shadow: 0px 0px 5px #545252;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
    -moz-box-shadow: 0px 0px 5px #545252;  /* Firefox 3.5 - 3.6 */
    box-shadow: 0px 0px 5px #545252;
}
.controlCol {
    font-weight: bold;
    text-align: right;
}
#controlBox {
    margin-top: 10px;
    background: -webkit-linear-gradient(#FAFAFA, #BEBEBE); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#FAFAFA, #BEBEBE); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#FAFAFA, #BEBEBE); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#FAFAFA, #BEBEBE); /* Standard syntax */
    -webkit-box-sizing : border-box;‌​
    -moz-box-sizing : border-box;
    box-sizing : border-box;
    height: auto;
    width: auto;
    -webkit-box-shadow: 0px 0px 5px #545252;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
    -moz-box-shadow: 0px 0px 5px #545252;  /* Firefox 3.5 - 3.6 */
    box-shadow: 0px 0px 5px #545252;
    display: inline-block;
    position: absolute;
    cursor: move;
    z-index: 9999;
}
audio {
    max-height: 95%;
}
.button {
    background: -webkit-linear-gradient(#FAFAFA, #BEBEBE); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#FAFAFA, #BEBEBE); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#FAFAFA, #BEBEBE); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#FAFAFA, #BEBEBE); /* Standard syntax */
    font-size: 18px;
    font-weight: bold;
    padding-top: 10px;
    padding-bottom: 10px;
    padding-left: 20px;
    padding-right: 20px;
}
#btnClipboard {
    margin-top: 15px;
}
#saveClipContainer {
    text-align: center;
    width: 100%;
}
textarea {
    min-height: 400px;
}
#lastSaved {
    color: red;
    text-align: right;
    width: 100%;
}
.heading {
    font-family: Arial, "Helvetica Neue", Helvetica, sans-serif;
    font-size: 14px;
    font-weight: bold;
    padding: 10px;
    vertical-align: middle;
    display:table-cell;
}
.headingDiv {
    background-color: #4C4C99;
    color: white;
    width: 100%;
}
.innerDiv {
    padding: 10px;
}
.controlBtn {
    cursor: pointer;
}
#btnPlay {
    margin-left: 10px;
    margin-right: 10px;
}
.audioBox {
    background: -webkit-linear-gradient(#FAFAFA, #BEBEBE); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#FAFAFA, #BEBEBE); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#FAFAFA, #BEBEBE); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#FAFAFA, #BEBEBE); /* Standard syntax */
    -webkit-box-sizing : border-box;‌​
    -moz-box-sizing : border-box;
    box-sizing : border-box;
    height: auto;
    width: auto;
    -webkit-box-shadow: 0px 0px 5px #545252;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
    -moz-box-shadow: 0px 0px 5px #545252;  /* Firefox 3.5 - 3.6 */
    box-shadow: 0px 0px 5px #545252;
}
.stdBox {
    background: -webkit-linear-gradient(#FAFAFA, #BEBEBE); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#FAFAFA, #BEBEBE); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#FAFAFA, #BEBEBE); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#FAFAFA, #BEBEBE); /* Standard syntax */
    -webkit-box-sizing: border-box;‌​
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    height: auto;
    width: auto;
    -webkit-box-shadow: 0px 0px 5px #545252;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
    -moz-box-shadow: 0px 0px 5px #545252;  /* Firefox 3.5 - 3.6 */
    box-shadow: 0px 0px 5px #545252;
    margin-bottom: 10px;
}
select {
    height: 25px;
    width: 100%;
}
.b {
    font-weight: bold;
    height: 40px;
    width: 150px;
}
#waveform {
    position: relative;
}
#progress-bar {
    position: absolute;
    z-index: 10;
    top: 50%;
    margin-top: -10px;
    left: 5%;
    width: 90%;
}
#audioTime {
    font-weight: bold;
    margin-bottom: 0;
}

.ui-dialog-titlebar {
    font-size: 14px;
    font-weight: bold;
}

.ui-dialog-content {
    font-size: 14px;
}

.ui-dialog-buttonpane {
    font-size: 14px;
    font-weight: bold;
}